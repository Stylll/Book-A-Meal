{
  "name": "book-a-meal",
  "version": "1.0.0",
  "description": "An application where customers can make food orders and vendors can know what a customer wants to eat",
  "main": "index.js",
  "scripts": {
    "migrate:db-prod": "sequelize db:migrate:undo:all --env=production && sequelize db:migrate --env=production && sequelize db:seed:all --env=production",
    "migrate:db-dev": "sequelize db:migrate:undo:all --env=development && sequelize db:migrate --env=development && sequelize db:seed:all --env=development",
    "migrate:db": "sequelize db:migrate:undo:all --env=test && sequelize db:migrate --env=test && sequelize db:seed:all --env=test",
    "pretest": "npm run migrate:db",
    "test:server": "npm run pretest && NODE_ENV=test nyc --reporter=html --reporter=text --reporter=lcov mocha --compilers js:babel-core/register server/tests/**/*.test.js --timeout 10000 --exit",
    "test:client": "jest /client",
    "start": "babel-node ./server/server",
    "start:dev": "nodemon --inspect --watch server --exec babel-node ./server/server --trace-warnings",
    "start:app": "babel-node ./tools/srcServer.js",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint \"server/**/*.js\" \"client/src/**/*.jsx\" \"client/src/**/*.js\" app.js --fix",
    "heroku:server": "babel-node ./server/server.js",
    "heroku-postbuild": "npm run build",
    "heroku:app": "babel-node tools/distServer.js",
    "prod:build": "npm run build",
    "clean-dist": "npm run remove-dist && mkdirp client/dist",
    "remove-dist": "rimraf client/dist",
    "build:html": "babel-node tools/buildHtml.js",
    "prebuild": "npm-run-all clean-dist build:html",
    "build": "babel-node tools/build.js",
    "open:build": "babel-node tools/distServer.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Stylll/Book-A-Meal.git"
  },
  "nyc": {
    "exclude": [
      "server/tests",
      "UI",
      "client",
      "client/__tests__",
      "client/dist",
      "tools"
    ]
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "client/src/**/*.{js,jsx}",
      "!client/src/store/*",
      "!client/src/{index,routes}.jsx",
      "!client/src/components/Main.jsx",
      "!client/src/reducers/{index,initialState}.js"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss|png|jpg|svg|gif)$": "<rootDir>./client/__tests__/__mocks__/importMock.js"
    },
    "testPathIgnorePatterns": [
      "client/__tests__/setup.js",
      "client/__tests__/helpers",
      "client/__tests__/__mocks__/*",
      "client/src/store/*"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>./client/__tests__/setup.js"
  },
  "keywords": [
    "Book",
    "Meal",
    "Food"
  ],
  "author": "Stephen Aribaba",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Stylll/Book-A-Meal/issues"
  },
  "homepage": "https://github.com/Stylll/Book-A-Meal#readme",
  "dependencies": {
    "app-root-path": "^2.1.0",
    "axios": "^0.18.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "bcrypt": "^2.0.1",
    "body-parser": "^1.18.2",
    "chai": "^4.1.2",
    "cheerio": "^1.0.0-rc.2",
    "cloudinary": "^1.11.0",
    "colors": "^1.2.5",
    "compression": "^1.7.2",
    "create-array": "^1.1.0",
    "css-loader": "^0.28.11",
    "dotenv": "^5.0.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "jest": "^22.4.4",
    "jsonwebtoken": "^8.2.1",
    "lodash": "^4.17.10",
    "mocha": "^5.1.1",
    "moment": "^2.22.1",
    "morgan": "^1.9.0",
    "multer": "^1.3.0",
    "node-sass": "^4.9.0",
    "node-schedule": "^1.3.0",
    "nodemailer": "^4.6.4",
    "npm-run-all": "^4.1.3",
    "open": "0.0.5",
    "path": "^0.12.7",
    "pg": "^6.4.2",
    "pg-hstore": "^2.3.2",
    "prop-types": "^15.6.1",
    "randomstring": "^1.1.5",
    "react": "^16.3.2",
    "react-confirm-alert": "^2.0.2",
    "react-dom": "^16.3.2",
    "react-paginate": "^5.2.3",
    "react-redux": "^5.0.7",
    "react-redux-loading-bar": "^4.0.5",
    "react-redux-toastr": "^7.2.6",
    "react-router-dom": "^4.2.2",
    "react-router-enzyme-context": "^1.2.0",
    "react-tooltip": "^3.6.1",
    "redux": "^4.0.0",
    "redux-mock-store": "^1.5.1",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^7.0.1",
    "sequelize": "^4.37.6",
    "sequelize-cli": "4.0.0",
    "style-loader": "^0.21.0",
    "supertest": "^3.0.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "url-loader": "^1.0.1",
    "validator": "^9.4.1",
    "webpack": "^4.8.3",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.22.2",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "coveralls": "^3.0.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.8.2",
    "istanbul": "^0.4.5",
    "moxios": "^0.4.0",
    "nodemon": "^1.17.3",
    "nyc": "^11.7.1",
    "rimraf": "^2.6.2"
  }
}
